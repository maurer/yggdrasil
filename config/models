Jot
    body Text
    created   UTCTime
    completed UTCTime Maybe
    deleted   UTCTime Maybe
    deriving Typeable

Task
    title Text
    completion Text
    created UTCTime
    completed UTCTime Maybe
    exec Bool
    delay UTCTime Maybe
    deriving Typeable

TaskDependency
    parent TaskId
    child  TaskId
    deriving Typeable
